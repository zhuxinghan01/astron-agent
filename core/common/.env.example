# Service Information
# Basic service identification and network configuration
SERVICE_SUB=spf
SERVICE_NAME=WorkFlow
SERVICE_LOCATION=hf
SERVICE_PORT=7880

# CACHE
REDIS_CLUSTER_ADDR=
REDIS_PASSWORD=
REDIS_EXPIRE=60

# DATABASE
MYSQL_HOST=
MYSQL_PORT=
MYSQL_USER=
MYSQL_PASSWORD=
MYSQL_DB=

# KAFKA
KAFKA_ENABLE=0
KAFKA_SERVERS=
KAFKA_TIMEOUT=10

# LOG
LOG_PATH="./openstellar_log"
LOG_LEVEL="ERROR"

# MASDK (Model as SDK) Configuration
# Service discovery and model integration settings for distributed AI services
# MASDK_SWITCH=1
MASDK_POLARIS_URL=
MASDK_POLARIS_PROJECT=
MASDK_POLARIS_GROUP=
MASDK_POLARIS_SERVICE=
MASDK_POLARIS_VERSION=
MASDK_CHANNEL=
MASDK_FUNCTION=

# OSS
# =============================================================================
# Object Storage Configuration
# =============================================================================

# Object Storage Service Settings
# Storage type options: ifly_gateway_storage (iFlytek), s3 (Amazon S3 compatible)
OSS_TYPE=
OSS_ENDPOINT=
OSS_ACCESS_KEY_ID=
OSS_ACCESS_KEY_SECRET=
OSS_BUCKET_NAME=
# Download domain for S3, required when using S3 storage type
OSS_DOWNLOAD_HOST=
# File validity period for iFlytek object storage (in seconds)
OSS_TTL=157788000

# OTLP
# 上报地址
OTLP_ENDPOINT=
# 上报开关
OTLP_ENABLE=1

# OTLP METRIC
# sdk上报metric时间间隔 建议小于30000ms  默认1000ms
OTLP_METRIC_EXPORT_INTERVAL_MILLIS=3000
# 默认配置 metrics上报服务端超时时间 单位ms 默认5000ms
OTLP_METRIC_EXPORT_TIMEOUT_MILLIS=3000
# 默认配置 与服务端建连时间 单位ms 默认5000ms
OTLP_METRIC_TIMEOUT=3000

# OTLP TRACE
OTLP_TRACE_TIMEOUT=3000
OTLP_TRACE_MAX_QUEUE_SIZE=2048
OTLP_TRACE_SCHEDULE_DELAY_MILLIS=3000
OTLP_TRACE_MAX_EXPORT_BATCH_SIZE=512
OTLP_TRACE_EXPORT_TIMEOUT_MILLIS=3000

# CONFIG - 需要配置在容器环境中
CONFIG_ENV_PATH=./.env
POLARIS_ENABLED=false
POLARIS_BASE_URL=
POLARIS_USERNAME=
POLARIS_PASSWORD=
POLARIS_PROJECT=
POLARIS_CLUSTER=
POLARIS_SERVICE=
POLARIS_VERSION=1.0.0
POLARIS_CONFIG_FILE=
POLARIS_RETRY_COUNT=3
POLARIS_RETRY_INTERVAL=10

# AUDIT
IFLYTEK_AUDIT_APP_ID=
IFLYTEK_AUDIT_ACCESS_KEY_ID=
IFLYTEK_AUDIT_ACCESS_KEY_SECRET=
IFLYTEK_AUDIT_HOST=